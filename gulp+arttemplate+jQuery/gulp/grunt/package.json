  "scripts": {
    "gulp-dev-mac": "export NODE_ENV=development&&gulp dev -p --progress --colors",
    "gulp-dev-windows": "set NODE_ENV=development&&gulp dev -p --progress --colors",
    "gulp-pro-mac": "export NODE_ENV=production&&gulp release -p --progress --colors",
    "gulp-pro-windows": "set NODE_ENV=production&&gulp release -p --progress --colors",
    "webpack-dev-mac": "export NODE_ENV=development&&webpack --display-error-details  -p --progress --colors",
    "webpack-dev-windows": "set NODE_ENV=development&&webpack --display-error-details  -p --progress --colors",
    "webpack-pro-mac": "export NODE_ENV=production&&webpack -p --progress --colors",
    "webpack-pro-windows": "set NODE_ENV=production&&webpack -p --progress --colors",
    "watch": "webpack --watch --progress --colors ",
    "webpack-dev-server": "webpack-dev-server --hot --inline --progress "
  },
