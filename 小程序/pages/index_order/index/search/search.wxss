.guigeBox {
  background: #fff;
  width: 80%;
  margin-left: 10%;
  margin-top: 20%;
  box-sizing: border-box;
  padding: 20rpx;
  border-radius: 6rpx;
}

.ggview1 {
  display: flex;
  justify-content: space-between;
}

.ggview1 text {
  font-size: 30rpx;
}

.ggyueshou {
  font-size: 24rpx;
  color: #999;
}

.ggChose {
  font-size: 30rpx;
  margin-top: 30rpx;
  padding-bottom: 24rpx;
  border-bottom: 1rpx solid #e5e5e5;
}
.gg-item{
   padding:20rpx 0rpx;
   display: flex;
   flex-wrap: wrap;
}
.wid20{
   width:18%;
}
.wid80{
   width:80%;
}


.ggChose .ggsize {
  padding: 4rpx 12rpx;
  border: 1rpx solid #999;
  border-radius: 24rpx;
  margin-right: 16rpx;
  margin-bottom: 16rpx;
  display: inline-block;
}

.ggChose .ggsizeon {
  border-color: #fe5064;
  color: #fe5064;
}

.ggbot {
  display: flex;
  justify-content: space-between;
  font-size: 30rpx;
  margin-top: 20rpx;
}

.ggbot .showPrice {
  color: #fe5064;
}

.ggbot .addCartBtn {
  background: #fe5064;
  font-size: 0.8rem;
  text-shadow: none;
  color: #fff;
  border-radius: 10rpx;
  font-weight: 100;
  padding: 6rpx 12rpx;
}




.banTit{
   font-size: 30rpx;
   position:relative;
   top:-5px;
}
.gudingTit{
  position:absolute;
  z-index:1;
  background: #fff;
  left:18rpx;
  right:0rpx;
  font-size:30rpx;
}
.no-result{
   width:100%;
   margin-top:40%;
}
.no-result view{
   text-align: center; 
   font-size: 28rpx; 
}
.no-result .noresImg{
    width:100rpx;
    height:80rpx;
}
.it-name{
   font-size: 30rpx;
}
.it-desc .red{
   font-size:26rpx;
}